{
  "name": "react-typescript-library-storybook-template",
  "version": "1.0.1",
  "keywords": [
    "react",
    "template",
    "packager",
    "typescript",
    "storybook",
    "UI library"
  ],
  "license": "MIT",
  "author": "Alan Zou",
  "main": "./index.js",
  "module": "./es6/index.js",
  "typings": "./index.d.ts",
  "scripts": {
    "build:lib": "concurrently \"npm run build:lib:common\" \"npm run build:lib:es6\"",
    "build:lib:common": "tsc --project tsconfig.prod.json",
    "build:lib:es6": "tsc --project tsconfig.prod.es6.json",
    "build:lib:test": "tsc --p tsconfig.prod.json --noEmit",
    "prebuild:storybook": "concurrently \"npm run lint:ts\" \"npm run lint:packageJson\" \"npm run test\" \"npm run build:lib:test\"",
    "build:storybook": "build-storybook -c .storybook -o public --no-dll --config-dir ./.storybook.doc",
    "clean": "rm -rf node_modules",
    "clean-cache": "rm -rf node_modules/.cache",
    "commit": "yarn git-cz",
    "create-component": "ts-node scripts/generator",
    "lint:packageJson": "eslint ./package.json --no-ignore",
    "lint:ts": "eslint '**/src/**/*.{ts,tsx,json}'",
    "package": "rm -rf dist && npm run build:lib",
    "postpackage": "node scripts/release.js",
    "release-after": "npm run update-mdx && ts-node scripts/mergeIntoPreviousCommit && npm run start-next",
    "release-major": "node scripts/release/majorRelease.run.js",
    "postrelease-major": "npm run release-after",
    "release-minor": "node scripts/release/minorRelease.run.js",
    "postrelease-minor": "npm run release-after",
    "release-patch": "node scripts/release/patchRelease.run.js",
    "postrelease-patch": "npm run release-after",
    "start": "npm run storybook",
    "start-next": "node scripts/release/start-next",
    "storybook": "start-storybook -p 6006 --no-manager-cache",
    "storybook:docgen": "start-storybook -p 6006 --config-dir ./.storybook.doc --no-dll --no-manager-cache",
    "test": "jest",
    "update-dep": "ncu --dep prod -u",
    "update-dev": "ncu --dep dev -u",
    "update-jest": "ncu '/jest/' -u",
    "update-mdx": "ts-node scripts/mdToMdx",
    "update-packageJson": "eslint ./package.json --no-ignore --fix",
    "update-snapshot": "jest --updateSnapshot",
    "update-storybook": "ncu '/storybook/' -u",
    "update-storybook-snapshot": "jest src/__tests__/snapshot/storyShots.test.tsx --updateSnapshot"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run prebuild:storybook"
    }
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    }
  },
  "dependencies": {},
  "devDependencies": {
    "@storybook/addon-a11y": "6.1.21",
    "@storybook/addon-docs": "6.1.21",
    "@storybook/addon-essentials": "6.1.21",
    "@storybook/addon-knobs": "6.1.21",
    "@storybook/addon-storyshots": "6.1.21",
    "@storybook/addon-storysource": "6.1.21",
    "@storybook/addons": "6.1.21",
    "@storybook/react": "6.1.21",
    "@storybook/theming": "6.1.21",
    "@types/fs-extra": "9.0.8",
    "@types/jest": "26.0.20",
    "@types/node": "^14.14.31",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.1",
    "@types/react-test-renderer": "17.0.1",
    "@typescript-eslint/eslint-plugin": "4.16.1",
    "@typescript-eslint/parser": "4.16.1",
    "babel-plugin-require-context-hook": "1.0.0",
    "circular-dependency-plugin": "5.2.2",
    "commitizen": "4.2.3",
    "concurrently": "6.0.0",
    "copy-webpack-plugin": "6.3.0",
    "core-js": "3.9.1",
    "cz-customizable": "6.3.0",
    "eslint": "7.21.0",
    "eslint-config-airbnb-typescript": "12.3.1",
    "eslint-config-prettier": "7.2.0",
    "eslint-loader": "4.0.2",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jest": "24.1.5",
    "eslint-plugin-json-format": "2.0.1",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-prettier": "3.3.1",
    "eslint-plugin-react": "7.22.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "eslint-plugin-security": "1.4.0",
    "fs-extra": "9.1.0",
    "husky": "5.1.3",
    "jest": "26.6.3",
    "jest-html-reporters": "2.1.2",
    "npm-check-updates": "11.1.10",
    "prettier": "1.19.1",
    "raf": "3.4.1",
    "react": "^17.0.1",
    "react-docgen": "5.3.1",
    "react-dom": "^17.0.1",
    "react-test-renderer": "17.0.1",
    "speed-measure-webpack-plugin": "1.4.2",
    "standard-version": "9.1.1",
    "thread-loader": "3.0.1",
    "ts-jest": "26.5.3",
    "ts-loader": "8.0.17",
    "ts-node": "9.1.1",
    "typescript": "4.2.3",
    "webpack": "4.23.1"
  },
  "peerDependencies": {
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  }
}
